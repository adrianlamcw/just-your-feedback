		-webkit-animation: reveal-header 0.5s ease;
		-ms-animation: reveal-header 0.5s ease;
		animation: reveal-header 0.5s ease;
	}




/* Menu */


	#menu > ul {
		margin: 0 0 1em 0;
	}

		#menu > ul.links {
			list-style: none;
			padding: 0;
		}

			#menu > ul.links > li {
				padding: 0;
			}

